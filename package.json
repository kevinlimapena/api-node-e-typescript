{
  "name": "api-node",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "tsx src/index.ts",
    "test": "jest",
    "knex:migrate": "knex --knexfile ./src/servidor/database/knex/Environment.ts migrate:latest",
    "knex:rollback-all": "knex --knexfile ./src/servidor/database/knex/Environment.ts migrate:rollback --all",
    "knex:rollback": "knex --knexfile ./src/servidor/database/knex/Environment.ts migrate:rollback",
    "knex:seed": "knex --knexfile ./src/servidor/database/knex/Environment.ts seed:run"
  },
  "dependencies": {
    "@babel/preset-env": "^7.28.5",
    "@types/bcryptjs": "^2.4.6",
    "babel-jest": "^30.2.0",
    "bcryptjs": "^3.0.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "pg": "^8.16.3",
    "yup": "^1.7.1",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "sqlite3": "^5.1.7",
    "@eslint/js": "^9.39.2",
    "@jest/globals": "^30.2.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  }
}